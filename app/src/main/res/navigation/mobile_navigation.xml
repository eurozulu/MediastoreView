<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/menu_show_data">

    <fragment
        android:id="@+id/menu_action_columns"
        android:name="org.spoofer.mediastoreView.ui.columnselect.ColumnSelectFragment"
        tools:layout="@layout/fragment_columns">
        <argument
            android:name="group_name"
            android:defaultValue="All Files"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/menu_show_data"
        android:name="org.spoofer.mediastoreView.ui.ShowDataFragment"
        tools:layout="@layout/fragment_showdata">
    </fragment>

</navigation>